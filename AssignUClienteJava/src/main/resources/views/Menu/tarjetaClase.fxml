<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<StackPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.AssignU.controllers.Menu.TarjetaClaseController">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="342.0" style="-fx-background-color: #d7f2ff;">
         <children>
            <Label fx:id="lbNombreClase" text="Nombre de la clase">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
            <Label fx:id="lbNombreDocente" text="Nombre del docente">
               <font>
                  <Font size="23.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="30.0" />
               </VBox.margin>
            </Label>
            <Button mnemonicParsing="false" onAction="#btnVerClase" stylesheets="@../stylesheets/button_ok.css" text="Ver clase">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
            </Button>
         </children>
         <StackPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </StackPane.margin>
      </VBox>
   </children>
</StackPane>
